		"node": "21.4.0",
		"npm": "10.2.5"
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"deploy-clear": "aws s3 rm s3://directorytospotify.s3.ap-southeast-2.amazonaws.com --recursive",
		"deploy-sync": "aws s3 sync --cache-control max-age=31536000 ./dist/ s3://directorytospotify.s3.ap-southeast-2.amazonaws.com",
		"deploy": "npm run build && npm run deploy-clear && npm run deploy-sync"
	},
